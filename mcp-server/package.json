{
  "name": "umbraco-mcp",
  "version": "1.0.0",
  "type": "module",
  "description": "A model context protocol (MCP) server for Umbraco",
  "main": "index.js",
  "bin": {
    "umbraco": "./build/index.js"
  },
  "scripts": {
    "clean": "rimraf build",
    "build2": "npm run clean && tsc",
    "build": "tsup ./src/index.ts",
    "watch": "tsup ./src/index.ts --watch",
    "generate": "orval --config orval.config.cjs",
    "inspect": "npx @modelcontextprotocol/inspector node build/index.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js"
  },
  "files": [
    "build"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Matthew-Wise/umbraco-mcp.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Matthew-Wise/umbraco-mcp/issues"
  },
  "homepage": "https://github.com/Matthew-Wise/umbraco-mcp#readme",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.9.0",
    "@types/uuid": "^10.0.0",
    "axios": "^1.8.4",
    "form-data": "^4.0.2",
    "qs": "^6.14.0",
    "rimraf": "^6.0.1",
    "uuid": "^11.1.0",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.1",
    "@types/qs": "^6.9.18",
    "dotenv": "^16.5.0",
    "jest": "^29.7.0",
    "jest-extended": "^4.0.2",
    "orval": "^7.8.0",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3"
  }
}
