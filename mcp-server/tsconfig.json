{
  "compilerOptions": {
    "target": "ES2022",
    "module": "Node16",
    "moduleResolution": "Node16",
    "outDir": "./build",
    "rootDir": "./src",
    "baseUrl": "./src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "paths": {
      "@/clients/*": ["./clients/*"],
      "@/helpers/*": ["./helpers/*"],
      "@/umb-management-api/*": ["./api/umbraco/management/*"],
    },
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
